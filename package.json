    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "ng-packagr": "^18.2.1",
    "protractor": "~7.0.0",
    "raw-loader": "^4.0.2",
    "standard-changelog": "^2.0.11",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "5.4"
  }
